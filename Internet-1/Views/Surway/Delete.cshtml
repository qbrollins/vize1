@{
    Layout = "_LayoutAdmin";
}
@section CardHeader{
    <span>Anket Sil</span>
}
@model             Internet_1.ViewModels.SurwayModel

<h4>@Model.Name adlı Anket silinecektir, Onaylıyor musunuz?</h4>

<form method="post" asp-action="Delete">
    <input type="hidden" asp-for="Id" />
    <input type="submit" value="Sil" class="btn btn-danger btn-sm" />
    <a asp-action="Index" class="btn btn-primary btn-sm">Vazgeç</a>
</form>


